{
  "name": "res-json",
  "version": "1.0.0",
  "description": "Node.js middleware to send JSON/JSONP responses",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "0.8"
          },
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "engines": {
    "node": ">= 0.8"
  },
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel src -d lib",
    "prepublish": "npm run build",
    "start": "node lib/",
    "test-cov": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "test": "babel-node ./node_modules/.bin/_mocha"
  },
  "author": "N. Palani Kumanan",
  "repository": {
    "type": "git",
    "url": "https://github.com/palanik/res-json.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "isparta": "^4.0.0",
    "mocha": "^3.4.2"
  }
}
